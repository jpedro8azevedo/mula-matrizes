import java.util.Scanner;

class multMatrizes {
	public static void main(String[] args) {
        
        Scanner sc = new Scanner(System.in);
        String[] entrada = sc.nextLine().split(" ");
        int[] numEntrada = new int[entrada.length];
        for (int i = 0; i < entrada.length; i++) {
        	numEntrada[i] = Integer.parseInt(entrada[i]);
        }
        int i = 0;
        System.out.println(primeiroNegativo(numEntrada, i));      
    }
	
	public static String primeiroNegativo(int[] numEntrada, int i) {
		
		String out = "";
		if (i >= numEntrada.length) {
			return out += "-";
		} else if (numEntrada[i] < 0) {
			return out += numEntrada[i];
		} else {
			return primeiroNegativo(numEntrada, i + 1);
		}
		
	}
	
}
