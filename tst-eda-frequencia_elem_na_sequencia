import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        
        Scanner sc = new Scanner(System.in);
        String num = sc.nextLine();
        String[] entrada = sc.nextLine().split(" ");
        int frequencia = 0;
        for (String elem: entrada) {
            if (elem.equals(num)) {
                frequencia++;
            }
        }
        System.out.println(frequencia);
    }
}
